<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:controller="org.example.javafxtest.Controllers.DiceController"
          alignment="CENTER" hgap="10" vgap="10"
          xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/8.0.171"
        stylesheets="@/org/example/javafxtest/Styles/style.css">
            <TextField fx:id="score" styleClass="Scores" editable="false" GridPane.columnSpan="2" maxHeight="200"/>
            <Button fx:id="goDice" styleClass="DiceBtn" GridPane.rowIndex="1" GridPane.columnIndex="0"/>


</GridPane>
