<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="org.example.javafxtest.Controllers.MenuController"
          hgap="10" vgap="10"
          alignment="CENTER"
          stylesheets="../Styles/MenuStyle.css">
    <Text fx:id="MenuTitle" text="Menu Title"  GridPane.columnSpan="3" />
    <Button fx:id="CalcBtn" text="Calc" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Button fx:id="DiceBtn" text="Dice" GridPane.columnSpan="2" GridPane.rowIndex="2"/>
</GridPane>

